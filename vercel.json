{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/studio", "destination": "/studio.html" },
    { "source": "/studio/", "destination": "/studio.html" },
    { "source": "/((?!api/|assets/|studio\\.html|favicon|robots|manifest).*)", "destination": "/index.html" }
  ]
}