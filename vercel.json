{
  "headers": [
    {
      "source": "/(.*\\.tsx)",
      "headers": [
        { "key": "Content-Type", "value": "text/jsx; charset=utf-8" }
      ]
    },
    {
      "source": "/(.*\\.ts)",
      "headers": [
        { "key": "Content-Type", "value": "text/typescript; charset=utf-8" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}