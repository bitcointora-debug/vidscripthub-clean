{
  "version": 2,
  "framework": null,
  "headers": [
    {
      "source": "/(.*\\.tsx|.*\\.ts)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript; charset=utf-8" }
      ]
    }
  ],
  "functions": {
    "api/gemini-proxy.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}